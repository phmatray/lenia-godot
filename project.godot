anti_aliasing/quality/msaa_2d=1
textures/decals/filter=2
textures/light_projectors/filter=2
environment/defaults/default_clear_color=Color(0.05, 0.05, 0.08, 1)
anti_aliasing/quality/screen_space_aa=1
